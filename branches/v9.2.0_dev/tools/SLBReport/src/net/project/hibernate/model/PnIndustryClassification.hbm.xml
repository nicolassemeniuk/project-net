<class 
    name="net.project.hibernate.model.PnIndustryClassification" 
    table="PN_INDUSTRY_CLASSIFICATION"
    lazy="false"
>

    <id
        name="industryId"
        type="java.math.BigDecimal"
        column="INDUSTRY_ID"
    >
    
