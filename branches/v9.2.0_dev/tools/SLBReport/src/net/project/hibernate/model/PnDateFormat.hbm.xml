<class 
    name="net.project.hibernate.model.PnDateFormat" 
    table="PN_DATE_FORMAT"
    lazy="false"
>

    <id
        name="dateFormatId"
        type="java.math.BigDecimal"
        column="DATE_FORMAT_ID"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="formatString"
        type="java.lang.String"
        column="FORMAT_STRING"
        not-null="true"
        length="80"
    />
    <property
        name="display"
        type="java.lang.String"
        column="DISPLAY"
        not-null="true"
        length="80"
    />
    <property
        name="example"
        type="java.lang.String"
        column="EXAMPLE"
        length="80"
    />

