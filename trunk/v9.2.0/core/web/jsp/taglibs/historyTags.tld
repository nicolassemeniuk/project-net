<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib
  PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
  "http://java.sun.com/j2ee/dtds/web-jsptaglib_1_1.dtd">
<taglib>
    <tlibversion>1.0</tlibversion>
    <jspversion>1.1</jspversion>
	<shortname>history</shortname>
	<info>History tags for JSP</info>

    <!-- History Tag -->
	<tag>
		<name>history</name>
		<tagclass>net.project.taglibs.history.HistoryTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <info>Creates history object</info>
        <attribute>
            <name>displayHere</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>target</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>style</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>stylesheet</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <!-- Display Tag -->
    <tag>
        <name>display</name>
        <tagclass>net.project.taglibs.history.DisplayTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <info>Displays history</info>
    </tag>
    
    <!-- Business Tag -->
    <tag>
        <name>business</name>
        <tagclass>net.project.taglibs.history.BusinessTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <info>Adds business tag</info>
        <attribute>
            <name>show</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>display</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>displayToken</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>jspPage</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>queryString</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>active</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
