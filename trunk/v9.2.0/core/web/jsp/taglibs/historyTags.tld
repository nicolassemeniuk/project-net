        </attribute>
        <attribute>
            <name>active</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <!-- Module Tag -->
    <tag>
        <name>module</name>
        <tagclass>net.project.taglibs.history.ModuleTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <info>Adds module tag</info>
