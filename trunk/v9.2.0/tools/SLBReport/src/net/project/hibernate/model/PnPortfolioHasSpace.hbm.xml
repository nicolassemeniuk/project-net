        <key-property 
            name="portfolioId" 
            column="PORTFOLIO_ID" 
            type="java.math.BigDecimal"
            length="20"
        />
        <key-property 
            name="spaceId" 
            column="SPACE_ID" 
            type="java.math.BigDecimal"
            length="20"
        />
    </composite-id>    

    <property
        name="isPrivate"
        type="java.lang.Integer"
        column="IS_PRIVATE"
        length="1"
    />

    <!-- Associations -->
    <!-- derived association(s) for compound key -->
    <!-- bi-directional many-to-one association to PnPortfolio -->
